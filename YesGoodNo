local function DTWBYD_fake_script() -- ScreenGui.LoginTween 
	local script = Instance.new('Script', ScreenGui)

	local object = script.Parent.LoginGUI
	object:TweenPosition(UDim2.new(0.308,0,0.25,0),"Out","Bounce",1.5)
end
coroutine.wrap(DTWBYD_fake_script)()
