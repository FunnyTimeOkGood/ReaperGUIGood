-- Scripts:

local function RWAEOK_fake_script() -- LoginGUI.Draggable 
	local script = Instance.new('LocalScript', LoginGUI)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(RWAEOK_fake_script)()
local function NVYECBO_fake_script() -- LoginGUI.LoginScript 
	local script = Instance.new('LocalScript', LoginGUI)

	script.Parent.Login.MouseButton1Click:Connect(function()
		if script.Parent.Username.Text == "Reaper" and script.Parent.Password.Text == "Monako145" and script.Parent.Discord.Text == "" then
			script.Parent.Parent.PremiumGUI.Visible = true
