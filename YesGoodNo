			script.Parent.Parent.PremiumGUI.Visible = true
			script.Parent.Parent.LoginGUI.Visible = false
		elseif script.Parent.Username.Text == "Chummy" and script.Parent.Password.Text == "Chummy8606" and script.Parent.Discord.Text == "" then
			script.Parent.Parent.PremiumGUI.Visible = true
			script.Parent.Parent.LoginGUI.Visible = false
		elseif script.Parent.Username.Text == "Messi" and script.Parent.Password.Text == "2014" and script.Parent.Discord.Text == "8d982kw" then
			script.Parent.Parent.PremiumGUI.Visible = true
			script.Parent.Parent.LoginGUI.Visible = false
		elseif script.Parent.Username.Text == "Fash" and script.Parent.Password.Text == "Fashion1010" and script.Parent.Discord.Text == "3j628bn" then
			script.Parent.Parent.PremiumGUI.Visible = true
